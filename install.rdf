<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>fatfox@frecel.com</em:id>
    <em:version>1.0</em:version>
    <em:type>4</em:type>

    <!-- Target Application this theme can install into,
         with minimum and maximum supported versions. -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>29.0</em:minVersion>
        <em:maxVersion>43.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>FatFox</em:name>
    <em:internalName>fatfox</em:internalName>
    <em:description>A big firefox theme designed for touch devices</em:description>
    <em:creator>Artur Fracek</em:creator>
    <em:homepageURL>https://github.com/frecel/fatfox/</em:homepageURL>
  </Description>
</RDF>
